<Application x:Class="TimeKeeper.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TimeKeeper">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/GingerBudUI;component/GingerBudUI.xaml"/>

                <ResourceDictionary Source="Resources/Dictionaries/Borders.xaml"/>
                <ResourceDictionary Source="Resources/Dictionaries/Buttons.xaml"/>
                <ResourceDictionary Source="Resources/Dictionaries/Converters.xaml"/>
                <ResourceDictionary Source="Resources/Dictionaries/HeaderedContentControls.xaml"/>
                <ResourceDictionary Source="Resources/Dictionaries/Images.xaml"/>
                <ResourceDictionary Source="Resources/Dictionaries/StackPanels.xaml"/>
                <ResourceDictionary Source="Resources/Dictionaries/TextControls.xaml"/>
                <ResourceDictionary Source="Resources/Dictionaries/Typography.xaml"/>
                <ResourceDictionary Source="Resources/Dictionaries/Variables.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
