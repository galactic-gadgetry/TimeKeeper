<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    
    <!--
    Base Tan:       #EFD9B4
    Base Pink:      #D6A692
    Base Brown:     #A39081
    Base Blue:      #4D6160
    Base Black:     #292522
    -->
    
    <!-- #Colors -->
    <Color x:Key="TextFillColorPrimary">#828790</Color>
    
    <Color x:Key="WindowBackgroundColor">#292522</Color>
    
    <!-- #Window -->
    <SolidColorBrush x:Key="WindowBackground" Color="{StaticResource WindowBackgroundColor}"/>
    <SolidColorBrush x:Key="WindowForeground" Color="Red"/>

</ResourceDictionary>