    <Color x:Key="WindowBackgroundColorPrimary100">#313338</Color>
    <Color x:Key="WindowBackgroundColorPrimary90">#36363D</Color>

    <Color x:Key="SolidControlFillColorPrimary80">#80848E</Color>
    <Color x:Key="SolidControlFillColorPrimary90">#6D6D76</Color>
    <Color x:Key="SolidControlFillColorPrimary100">#4E5058</Color>
    <Color x:Key="SolidControlFillColorPrimary120">#404249</Color>
    <Color x:Key="SolidControlFillColorPrimary130">#2B2D31</Color>
    <Color x:Key="SolidControlFillColorPrimaryDisabled">#494A50</Color>

    <Color x:Key="SolidControlFillColorSecondary100">#5865F2</Color>
    <Color x:Key="SolidControlFillColorSecondary110">#4752C4</Color>
    <Color x:Key="SolidControlFillColorSecondary120">#3C45A5</Color>
    <Color x:Key="SolidControlFillColorSecondaryDisabled">#424991</Color>

    <Color x:Key="TextFillColorPrimary80">#C1C3C7</Color>
    <Color x:Key="TextFillColorPrimary90">#B5BAC1</Color>
    <Color x:Key="TextFillColorPrimary100">#828790</Color>

    <Color x:Key="TextFillColorSecondary100">#FFFFFF</Color>
    <Color x:Key="TextFillColorSecondary120">#E0E1E3</Color>
    <Color x:Key="TextFillColorSecondaryDisabled">#A3A4A7</Color>

    <!-- #Brushes -->
    <SolidColorBrush x:Key="LayerFillColorBrush1" Color="{StaticResource WindowBackgroundColorPrimary100}"/>
    <SolidColorBrush x:Key="LayerFillColorBrush2" Color="{StaticResource SolidControlFillColorPrimary130}"/>

    <SolidColorBrush x:Key="TextAccentFillColorBrush1" Color="{StaticResource TextFillColorPrimary90}"/>


    <!-- #Button -->
    <SolidColorBrush x:Key="ButtonBackground" Color="{StaticResource SolidControlFillColorPrimary100}"/>
    <SolidColorBrush x:Key="ButtonBackgroundMouseOver" Color="{StaticResource SolidControlFillColorPrimary90}"/>
    <SolidColorBrush x:Key="ButtonBackgroundPressed" Color="{StaticResource SolidControlFillColorPrimary80}"/>
    <SolidColorBrush x:Key="ButtonBackgroundDisabled" Color="{StaticResource SolidControlFillColorPrimaryDisabled}"/>
    <SolidColorBrush x:Key="ButtonForeground" Color="{StaticResource TextFillColorSecondary100}"/>
    <SolidColorBrush x:Key="ButtonForegroundDisabled" Color="{StaticResource TextFillColorSecondaryDisabled}"/>
    <SolidColorBrush x:Key="ButtonBorderBrush" Color="{StaticResource TextFillColorSecondary100}"/>
    <SolidColorBrush x:Key="ButtonBorderBrushMouseOver" Color="{StaticResource TextFillColorSecondary100}"/>
    <SolidColorBrush x:Key="ButtonBorderBrushPressed" Color="{StaticResource TextFillColorSecondary100}"/>
    <SolidColorBrush x:Key="ButtonBorderBrushDisabled" Color="{StaticResource TextFillColorSecondaryDisabled}"/>

    <SolidColorBrush x:Key="AccentButtonBackground" Color="{StaticResource SolidControlFillColorSecondary100}"/>
    <SolidColorBrush x:Key="AccentButtonBackgroundMouseOver" Color="{StaticResource SolidControlFillColorSecondary110}"/>
    <SolidColorBrush x:Key="AccentButtonBackgroundPressed" Color="{StaticResource SolidControlFillColorSecondary120}"/>
    <SolidColorBrush x:Key="AccentButtonBackgroundDisabled" Color="{StaticResource SolidControlFillColorSecondaryDisabled}"/>
    <SolidColorBrush x:Key="AccentButtonBorderBrushMouseOver" Color="{StaticResource TextFillColorSecondary100}"/>
    <SolidColorBrush x:Key="AccentButtonBorderBrushPressed" Color="{StaticResource TextFillColorSecondary100}"/>
    <SolidColorBrush x:Key="AccentButtonBorderBrushDisabled" Color="{StaticResource TextFillColorSecondaryDisabled}"/>


    <SolidColorBrush x:Key="NavigationBarButtonBackground" Color="Transparent"/>
    <SolidColorBrush x:Key="NavigationBarButtonBackgroundMouseOver" Color="{StaticResource WindowBackgroundColorPrimary90}"/>
    <SolidColorBrush x:Key="NavigationBarButtonBackgroundPressed" Color="{StaticResource SolidControlFillColorPrimary100}"/>
    <SolidColorBrush x:Key="NavigationBarButtonBackgroundDisabled" Color="{StaticResource SolidControlFillColorPrimaryDisabled}"/>
    <SolidColorBrush x:Key="NavigationBarButtonBackgroundHighlighted" Color="{StaticResource SolidControlFillColorPrimary120}"/>
    <SolidColorBrush x:Key="NavigationBarButtonForeground" Color="{StaticResource TextFillColorPrimary80}"/>
    <SolidColorBrush x:Key="NavigationBarButtonForegroundMouseOver" Color="{StaticResource TextFillColorSecondary120}"/>
    <SolidColorBrush x:Key="NavigationBarButtonForegroundPressed" Color="{StaticResource TextFillColorSecondary100}"/>
    <SolidColorBrush x:Key="NavigationBarButtonForegroundDisabled" Color="{StaticResource TextFillColorSecondaryDisabled}"/>
    <SolidColorBrush x:Key="NavigationBarButtonForegroundHighlighted" Color="{StaticResource TextFillColorSecondary100}"/>
    <SolidColorBrush x:Key="NavigationBarButtonBorderBrush" Color="{StaticResource TextFillColorSecondary100}"/>
    <SolidColorBrush x:Key="NavigationBarButtonBorderBrushMouseOver" Color="{StaticResource TextFillColorSecondary100}"/>
    <SolidColorBrush x:Key="NavigationBarButtonBorderBrushPressed" Color="{StaticResource TextFillColorSecondary100}"/>
    <SolidColorBrush x:Key="NavigationBarButtonBorderBrushDisabled" Color="{StaticResource TextFillColorSecondaryDisabled}"/>


    <!-- #Label -->
    <SolidColorBrush x:Key="LabelForeground" Color="{StaticResource TextFillColorPrimary100}"/>
    
    
    <!-- #TextBlock -->
    <SolidColorBrush x:Key="TextBlockForeground" Color="{StaticResource TextFillColorPrimary90}"/>


    <!-- #Window -->
    <SolidColorBrush x:Key="WindowBackground" Color="{StaticResource WindowBackgroundColorPrimary100}"/>
    <SolidColorBrush x:Key="WindowForeground" Color="{StaticResource TextFillColorPrimary100}"/>
    
    
    
    
    
    
    
